{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "inputs": [],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "demo": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": ["dist/**"],
      "persistent": true
    }
  }
}
