{
  "name": "@near-lake/monorepo",
  "author": "NEAR Inc <hello@nearprotocol.com>",
  "description": "A monorepo for Near Lake Framework and its related libraries",
  "private": true,
  "license": "(MIT OR Apache-2.0)",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/near/near-lake-framework-js.git"
  },
  "homepage": "https://near-indexers.io",
  "scripts": {
    "postinstall": "npm run build",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "demo": "turbo run demo --filter=@near-lake/$npm_config_name"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.6",
    "@changesets/cli": "^2.24.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^17.0.23",
    "jest": "^29.7.0",
    "prettier": "2.6.2",
    "ts-jest": "^29.1.1",
    "turbo": "^1.4.5",
    "typescript": "^4.6.4"
  }
}
